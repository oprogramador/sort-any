{
  "name": "sort-any",
  "version": "4.0.7",
  "description": "Sorts any JavaScript array in a predictable way (deep equal arrays are always sorted in the same order)",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build && babel src --out-dir build --ignore tests/",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "coverage": "nyc --check-coverage=true npm run test",
    "test": "mocha -r babel-register --recursive src/tests --timeout 5000"
  },
  "engines": {
    "node": ">=24.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oprogramador/sort-any.git"
  },
  "keywords": [
    "sort",
    "any",
    "array",
    "object",
    "deep",
    "equal",
    "predictable"
  ],
  "author": "oprogramador",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oprogramador/sort-any/issues"
  },
  "homepage": "https://github.com/oprogramador/sort-any#readme",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-register": "^6.26.0",
    "chai": "^6.2.2",
    "chai-as-promised": "^8.0.2",
    "chai-subset": "^1.6.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-improved": "^5.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "semantic-release": "25.0.3",
    "sinon": "^21.0.1",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^4.0.1"
  },
  "dependencies": {}
}
